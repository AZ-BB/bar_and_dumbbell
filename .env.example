# Copy this file to .env.local and update with your values

# WhatsApp Configuration
NEXT_PUBLIC_WHATSAPP_NUMBER=201234567890

# Google Maps Embed URL
NEXT_PUBLIC_GOOGLE_MAPS_EMBED=https://www.google.com/maps/embed?pb=...

# Email Configuration
NEXT_PUBLIC_EMAIL_ADDRESS=your-gym-email@example.com

# Supabase Configuration (for lead generation)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Admin auth (login for /admin, /admin/cms, /admin/contacts)
ADMIN_USERNAME=admin
ADMIN_PASSWORD=your_secure_password

# Key-Value store (CMS plans). Optional; falls back to data/plans.json when unset or KV unreachable.
KV_API_BASE_URL=https://your-kv-api.example.com
KV_API_KEY=your_api_key
KV_KEY=plans
